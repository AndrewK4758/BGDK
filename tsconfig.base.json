      "@bgdk/games-components-logic": ["packages/games-components-logic/src/index.ts"],
      "@bgdk/instance-of-game": ["packages/models/instance-of-game/src/index.ts"],
      "@bgdk/media-recorder": ["packages/media-recorder/src/index.ts"],
      "@bgdk/mocks": ["packages/mocks/src/index.ts"],
      "@bgdk/password": ["packages/password/src/index.ts"],
      "@bgdk/prisma": ["packages/prisma/src/index.ts"],
      "@bgdk/prompt-builder": ["packages/gen-ai/prompt-builder/src/index.ts"],
      "@bgdk/react-components": ["packages/react-components/src/index.ts"],
      "@bgdk/rule": ["packages/models/rule/src/index.ts"],
      "@bgdk/server": ["server/server/src/index.ts"],
      "@bgdk/shared-react-components": ["packages/shared-react-components/src/index.ts"],
