      "@bgdk/socket-io-server": ["packages/socket-io/server/src/index.ts"],
      "@bgdk/tic-tac-toe": ["packages/games/tic-tac-toe/src/index.ts"],
      "@bgdk/types-ai": ["packages/types/types-ai/src/index.ts"],
      "@bgdk/types-api": ["packages/types/types-api/src/index.ts"],
      "@bgdk/types-game": ["packages/types/types-game/src/index.ts"],
      "@bgdk/utils": ["packages/utils/src/index.ts"],
