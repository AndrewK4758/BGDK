    "../../libs/react-components/src/**/*.tsx",
    "../../libs/react-components/src/**/*.ts",
    "../../libs/chutes-and-ladders/src/**/*.ts",
    "../../libs/chutes-and-ladders/src/**/*.ts",
    "../../libs/models/game-builder/src/**/*.ts",
    "../../libs/chain/src/**/*.ts",
    "../../libs/game/src/**/*.ts",
    "../../libs/models/rule/src/**/*.ts",
    "../../libs/types/types-game/src/**/*.ts",
    "../../libs/games-components-logic/src/**/*.ts",
    "../../libs/chains-for-games/src/**/*.ts",
    "../../libs/models/all-games-map/src/**/*.ts",
    "../../libs/models/instance-of-game/src/**/*.ts",
    "../../libs/models/de-referencing-utilities/src/**/*.ts",
