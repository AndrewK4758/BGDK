{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "../../../dist/out-tsc",
    "types": ["node", "@nx/react/typings/cssmodule.d.ts", "@nx/react/typings/image.d.ts", "vite/client"],
    "target": "ESNext",
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "composite": true
  },
  "exclude": [
    "src/**/*.spec.ts",
    "src/**/*.test.ts",
    "src/**/*.spec.tsx",
    "src/**/*.test.tsx",
    "src/**/*.spec.js",
    "src/**/*.test.js",
    "src/**/*.spec.jsx",
    "src/**/*.test.jsx",
    "../../../libs/shared-react-components/src/tests/**/*.tsx",
    "../../../libs/shared-react-components/src/tests/**/*.ts",
    "../../../libs/react-components/src/tests/**/*.tsx",
    "../../../libs/react-components/src/tests/**/*.ts",
    "../../../libs/types/**/*/src/tests/**/*.ts",
    "../../../libs/models/**/*/src/tests/**/*.ts",
    "../../../libs/chain/src/tests/**/*.ts",
    "../../../libs/chains-for-games/src/tests/**/*.ts",
    "../../../libs/game/src/tests/**/*.ts",
    "../../../libs/games/**/*/src/tests/**/*.ts",
    "../../../libs/games-components-logic/src/tests/**/*.ts"
  ],
  "include": [
    "src/**/*.js",
    "src/**/*.jsx",
    "src/**/*.ts",
    "src/**/*.tsx",
    "../../../libs/shared-react-components/src/**/*.tsx",
    "../../../libs/shared-react-components/src/**/*.ts",
    "../../../libs/react-components/src/**/*.tsx",
    "../../../libs/react-components/src/**/*.ts",
    "../../../libs/types/**/*/src/**/*.ts",
    "../../../libs/models/**/*/src/**/*.ts",
    "../../../libs/chain/src/**/*.ts",
    "../../../libs/chains-for-games/src/**/*.ts",
    "../../../libs/game/src/**/*.ts",
    "../../../libs/games/**/*/src/**/*.ts",
    "../../../libs/games-components-logic/src/**/*.ts"
  ]
}
