{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "../../../dist/apps/local-model",
    "tsBuildInfoFile": "../../../dist/apps/local-model/tsconfig.app.tsbuildinfo",
    "types": ["node", "@nx/react/typings/cssmodule.d.ts", "@nx/react/typings/image.d.ts", "vite/client"],
    "module": "ESNext",
    "moduleResolution": "Bundler"
  },
  "include": [
    "src/**/*.js",
    "src/**/*.jsx",
    "src/**/*.ts",
    "src/**/*.tsx",
    "../../../packages/shared-react-components/src/**/*.ts",
    "../../../packages/shared-react-components/src/**/*.tsx",
    "../../../packages/socket-io/client/src/**/*.ts",
    "../../../packages/types/types-ai/src/**/*.ts"
  ],
  "exclude": [
    "src/**/*.spec.ts",
    "src/**/*.test.ts",
    "src/**/*.spec.tsx",
    "src/**/*.test.tsx",
    "src/**/*.spec.js",
    "src/**/*.test.js",
    "src/**/*.spec.jsx",
    "src/**/*.test.jsx",
    "vite.config.ts",
    "vite.config.mts",
    "vitest.config.ts",
    "vitest.config.mts"
  ],
  "references": []
}
