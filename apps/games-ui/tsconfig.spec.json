{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "../../dist/out-tsc",
    "types": ["jest", "node", "@nx/react/typings/cssmodule.d.ts", "@nx/react/typings/image.d.ts"],
    "esModuleInterop": true,
    "composite": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler"
  },
  "include": [
    "jest.config.ts",
    "src/tests/**/*.test.ts",
    "src/tests/**/*.spec.ts",
    "src/tests/**/*.test.tsx",
    "src/tests/**/*.spec.tsx",
    "src/tests/**/*.test.js",
    "src/tests/**/*.spec.js",
    "src/tests/**/*.test.jsx",
    "src/tests/**/*.spec.jsx",
    "src/tests/**/*.d.ts"
  ]
}
