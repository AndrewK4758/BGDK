{
  "name": "mocks",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/__mocks__/mocks/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "esbuildConfig": "libs/__mocks__/mocks/esbuild.config.cjs",
        "main": "libs/__mocks__/mocks/src/index.ts",
        "outputPath": "libs/__mocks__/mocks/dist",
        "outputFileName": "main.js",
        "tsConfig": "libs/__mocks__/mocks/tsconfig.lib.json",
        "assets": [
          {
            "glob": "libs/__mocks__/mocks/README.md",
            "input": ".",
            "output": "."
          }
        ],
        "deleteOutputPath": true,
        "metafile": true,
        "platform": "node",
        "sourcemap": "linked",
        "target": "node23",
        "format": ["esm", "cjs"],
        "bundle": true,
        "write": true
      },
      "configurations": {
        "development": {
          "minify": false
        },
        "production": {
          "minify": true
        }
      }
    },
    "typecheck": {
      "executor": "@nx/js:tsc",
      "outputs": ["{projectRoot}/dist/libs/__mocks__/moocks"],
      "options": {
        "outputPath": "dist/libs/__mocks__/mocks",
        "tsConfig": "tsconfig.base.json",
        "main": "libs/__mocks__/mocks/src/index.ts",
        "color": true
      }
    }
  }
}
