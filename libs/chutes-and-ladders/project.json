    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/chutes-and-ladders/**/*.{ts,tsx,js,jsx}", "libs/chutes-and-ladders/package.json"]
      }
