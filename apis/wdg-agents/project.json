{
  "name": "wdg-agents",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apis/wdg-agents/wdg_agents",
  "targets": {
    "lock": {
      "executor": "@nxlv/python:run-commands",
      "options": {
        "command": "poetry lock --no-update",
        "cwd": "apis/wdg-agents"
      }
    },
    "add": {
      "executor": "@nxlv/python:add",
      "options": {}
    },
    "update": {
      "executor": "@nxlv/python:update",
      "options": {}
    },
    "remove": {
      "executor": "@nxlv/python:remove",
      "options": {}
    },
    "build": {
      "executor": "@nxlv/python:build",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "outputPath": "apis/wdg-agents/dist",
        "publish": false,
        "lockedVersions": true,
        "bundleLocalDependencies": true
      }
    },
    "install": {
      "executor": "@nxlv/python:install",
      "options": {
        "silent": false,
        "args": "",
        "cacheDir": ".cache/pypoetry",
        "verbose": false,
        "debug": false
      }
    },
    "lint": {
      "executor": "@nxlv/python:flake8",
      "outputs": ["{workspaceRoot}/reports/apis/wdg-agents/pylint.txt"],
      "options": {
        "outputFile": "reports/apis/wdg-agents/pylint.txt"
      }
    },
    "start-poetry": {
      "executor": "@nxlv/python:run-commands",
      "options": {
        "command": "poetry shell",
        "cwd": "apis/wdg-agents"
      }
    },
    "test": {
      "executor": "@nxlv/python:run-commands",
      "outputs": ["{workspaceRoot}/reports/apis/wdg-agents/unittests", "{workspaceRoot}/coverage/apis/wdg-agents"],
      "options": {
        "command": "poetry run pytest tests/",
        "cwd": "apis/wdg-agents"
      }
    }
  },
  "tags": [],
  "release": {
    "version": {
      "generator": "@nxlv/python:release-version"
    }
  }
}
