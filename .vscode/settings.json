{
  "jest.enable": true,
  "jest.rootPath": "./",
  "jestrunner.jestPath": "./node_modules/jest/bin/jest",
  "jest.jestCommandLine": "pnpm",
  "jest.runMode": {
    "type": "on-demand",
    "showInlineError": true,
    "coverage": true
  },
  "jest.outputConfig": {
    "revealOn": "run",
    "revealWithFocus": "test-results",
    "clearOnRun": "none"
  },
  "jestrunner.codeLens": ["run", "debug", "coverage"],
  "testing.openTesting": "neverOpen"
}
